# linear search

use io
use lists

const numlist = [7, 1, 2, 5, 4, 6, 3]
const target = 4

proc findNum(numbers, num)
    let i = 0
    let count = len(numbers)

    while (i < count)
        if (at(numbers, i) == num)
            return i
        end

        set i = i + 1
    end

    return -1
end

